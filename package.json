{
  "name": "gulp-starter",
  "private": true,
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/shanewilson/gulp-starter.git"
  },
  "homepage": "https://github.com/shanewilson/gulp-starter",
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "dependencies": {
    "react": "0.10.0",
    "react-tools": "0.10.0"
  },
  "devDependencies": {
    "bower": "1.3.5",
    "browser-sync": "1.1.1",
    "compression-webpack-plugin": "0.1.1",
    "coverjs-loader": "0.5.0",
    "css-loader": "0.6.12",
    "file-loader": "0.5.1",
    "graceful-fs": "3.0.2",
    "gulp": "3.8.2",
    "gulp-cdnizer": "0.2.7",
    "gulp-html-prettify": "0.0.1",
    "gulp-if": "1.2.1",
    "gulp-jscs": "0.5.0",
    "gulp-jshint": "1.6.3",
    "gulp-load-plugins": "0.5.2",
    "gulp-minify-html": "0.1.3",
    "gulp-react": "0.4.0",
    "gulp-replace": "0.3.0",
    "gulp-rev": "0.4.1",
    "gulp-rimraf": "0.1.0",
    "gulp-task-listing": "0.3.0",
    "gulp-util": "2.2.18",
    "html-loader": "0.2.2",
    "jest-cli": "0.1.16",
    "jshint-stylish": "0.2.0",
    "jsx-loader": "0.10.2",
    "npm-check-updates": "1.2.0",
    "style-loader": "0.6.4",
    "stylus-loader": "0.3.0",
    "url-loader": "0.5.5",
    "webpack": "1.3.1-beta4"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "deps": "npm install --quiet --loglevel warn && ./node_modules/bower/bin/bower install --quiet",
    "upgrade": "./node_modules/npm-check-updates/bin/npm-check-updates -u && npm test",
    "test": "jest",
    "dev": "./node_modules/gulp/bin/gulp.js serve",
    "stage": "./node_modules/gulp/bin/gulp.js serve --production",
    "build": "./node_modules/gulp/bin/gulp.js build",
    "dist": "./node_modules/gulp/bin/gulp.js build --production"
  }
}
