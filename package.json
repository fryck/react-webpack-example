{
  "name": "react-webpack-example",
  "private": true,
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/shanewilson/react-webpack-example.git"
  },
  "homepage": "https://github.com/shanewilson/react-webpack-example",
  "author": {
    "name": "Shane Wilson",
    "email": "shanezilla@gmail.com",
    "url": "https://github.com/shanewilson/"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "testFileExtensions": [
      "js"
    ],
    "testDirectoryName": "unit",
    "testPathDirs": [
      "<rootDir>/tests/"
    ]
  },
  "dependencies": {
    "react": "0.12.2",
    "react-router": "0.11.6",
    "react-tools": "0.12.2"
  },
  "devDependencies": {
    "axis": "0.3.0",
    "babel": "^4.5.1",
    "babel-core": "^4.5.4",
    "babel-jest": "4.0.0",
    "babel-loader": "^4.0.0",
    "babel-runtime": "^4.5.5",
    "bower": "1.3.12",
    "compression-webpack-plugin": "0.1.2",
    "css-loader": "0.9.1",
    "expose-loader": "0.6.0",
    "flux": "^2.0.1",
    "graceful-fs": "3.0.5",
    "gulp": "3.8.10",
    "gulp-cdnizer": "1.1.2",
    "gulp-html-prettify": "0.0.1",
    "gulp-if": "1.2.5",
    "gulp-load-plugins": "0.8.0",
    "gulp-minify-html": "0.1.8",
    "gulp-replace": "0.5.2",
    "gulp-rev": "3.0.0",
    "gulp-rimraf": "0.1.1",
    "gulp-task-listing": "1.0.0",
    "gulp-util": "3.0.3",
    "html-loader": "0.2.3",
    "immutable": "3.6.2",
    "jeet": "6.1.2",
    "jest": "0.1.37",
    "jest-cli": "0.2.2",
    "jsx-loader": "0.12.2",
    "nightwatch": "0.5.36",
    "npm-check-updates": "1.5.1",
    "react-a11y": "0.0.6",
    "react-hot-loader": "1.1.4",
    "script-loader": "0.6.1",
    "selenium-standalone": "4.1.0",
    "style-loader": "0.8.3",
    "stylus-loader": "0.5.0",
    "url-loader": "0.5.5",
    "webpack": "1.7.1",
    "webpack-dev-server": "1.7.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "upgrade": "npm-check-updates -u && npm test",
    "test": "jest",
    "browser": "nightwatch -e firefox,chrome",
    "prestart": "npm install",
    "start": "webpack-dev-server --content-base src --hot --inline --https --colors -d --quiet --history-api-fallback",
    "prebuild": "npm install",
    "build": "rm -rf dist/ && BUILD_ENV=build webpack",
    "dist": "npm test && npm run build && npm run browser"
  }
}
