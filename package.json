{
  "name": "gulp-starter",
  "private": true,
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/shanewilson/gulp-starter.git"
  },
  "homepage": "https://github.com/shanewilson/gulp-starter",
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "dependencies": {
    "react": "0.10.0",
    "react-tools": "0.10.0",
    "react-nested-router": "0.2.1"
  },
  "devDependencies": {
    "bower": "1.3.8",
    "compression-webpack-plugin": "0.1.1",
    "css-loader": "0.7.0",
    "graceful-fs": "3.0.2",
    "gulp": "3.8.6",
    "gulp-cdnizer": "0.2.9",
    "gulp-html-prettify": "0.0.1",
    "gulp-if": "1.2.2",
    "gulp-load-plugins": "0.5.3",
    "gulp-minify-html": "0.1.4",
    "gulp-replace": "0.4.0",
    "gulp-rev": "0.4.2",
    "gulp-rimraf": "0.1.0",
    "gulp-task-listing": "0.3.0",
    "gulp-util": "2.2.20",
    "html-loader": "0.2.2",
    "jeet": "^5.3.0",
    "jest-cli": "0.1.17",
    "jshint": "2.5.2",
    "jshint-loader": "0.8.0",
    "jshint-stylish": "0.4.0",
    "jsx-loader": "0.10.2",
    "npm-check-updates": "1.2.0",
    "style-loader": "0.6.4",
    "stylus-loader": "0.3.1",
    "url-loader": "0.5.5",
    "webpack": "1.3.2-beta1",
    "webpack-dev-server": "1.4.7"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "clean": "rm -rf ./node_modules/ ./bower_components/",
    "deps": "npm install --quiet --loglevel warn && ./node_modules/bower/bin/bower install --quiet",
    "upgrade": "./node_modules/npm-check-updates/bin/npm-check-updates -u && npm test",
    "test": "jest",
    "serve": "./node_modules/gulp/bin/gulp.js serve",
    "build": "jest && ./node_modules/gulp/bin/gulp.js build",
    "dist": "jest && ./node_modules/gulp/bin/gulp.js build --production"
  }
}
